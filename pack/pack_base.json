{
  "ver": 1,
  "meta": {
    "name": "Default asset pack",
    "description": "Base asset pack for Dead on Arrival.",
    "release": "1.0",
    "author": "szym-mie",
    "email": "szym.mie@gmail.com"
  },
  "data": {
    "anim": {
      "#search": {
        "path": "anim/",
        "loader": "json",
        "load_later": true,
        "max_layers": 16
      }
    },
    "tex": {
      "#search": {
        "path": "tex/",
        "loader": "image",
        "load_later": false,
        "max_layers": 16
      }
    },
    "snd": {
      "#search": {
        "path": "snd/",
        "loader": "sound",
        "load_later": false,
        "max_layers": 16
      }
    },
    "stat": {
      "#search": {
        "path": "stat/",
        "loader": "json",
        "load_later": false,
        "max_layers": 16
      }
    },
    "gl": {
      "#search": {
        "path": "gl/",
        "loader": "text",
        "load_later": false,
        "max_layers": 4
      }
    },
    "lvl": {
      "#search": {
        "path": "lvl/",
        "loader": "image",
        "load_later": true,
        "max_layers": 16
      }
    }
  }
}